# Android fstab file.
#<src>                                         <mnt_point>  <type>  <mnt_flags and options>  <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

/dev/block/mmcblk0p35                           /system      ext4    noatime,barrier=0                                          wait
/dev/block/mmcblk0p36                           /cache       ext4    noatime,nosuid,nodev,barrier=0                             wait,check
/dev/block/mmcblk0p37                           /data        ext4    noatime,nosuid,nodev,noauto_da_alloc,barrier=0,discard     wait,check,encryptable=footer
