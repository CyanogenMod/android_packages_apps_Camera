# Master file for snd_soc_msm sound card
# Lists all use case verbs
SectionUseCase."HiFi" {
Comment "Music playback use case"
SectionVerb
	Name "HiFi"

	EnableSequence
		'SLIMBUS_0_RX Audio Mixer MultiMedia1':1:1
	EndSequence

	DisableSequence
		'SLIMBUS_0_RX Audio Mixer MultiMedia1':1:0
	EndSequence

	# ALSA PCMs
	CapturePCM 0
	PlaybackPCM 0

EndSection

